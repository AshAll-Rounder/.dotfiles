{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"final_space": true,
	"console_title": true,
	"console_title_style": "folder",
	"blocks": [
		{
			"type": "prompt",
			"alignment": "left",
			"segments": [
				{
					"type": "session",
					"style": "diamond",
					"foreground": "#ffffff",
					"background": "#c386f1",
					"leading_diamond": "ÓÇ∂",
					"trailing_diamond": "ÓÇ∞",
					"properties": {
						"postfix": " ",
						"display_host": false
					}
				},
				{
					"type": "path",
					"style": "powerline",
					"powerline_symbol": "ÓÇ∞",
					"foreground": "#ffffff",
					"background": "#ff479c",
					"properties": {
						"prefix": " Ôùä  ",
						"home_icon": "~",
						"folder_separator_icon": "\uE0b1",
						"style": "folder"
					}
				},
				{
					"type": "git",
					"style": "powerline",
					"powerline_symbol": "ÓÇ∞",
					"invert_powerline": false,
					"foreground": "#193549",
					"background": "#fffb38",
					"leading_diamond": "",
					"trailing_diamond": "",
					"properties": {
						"display_status": true,
						"display_stash_count": true,
						"display_upstream_icon": true
					}
				},
				{
					"type": "node",
					"style": "powerline",
					"powerline_symbol": "ÓÇ∞",
					"foreground": "#ffffff",
					"background": "#6CA35E",
					"properties": {
						"prefix": " \uE718 "
					}
				},
				{
					"type": "java",
					"style": "powerline",
					"powerline_symbol": "ÓÇ∞",
					"foreground": "#ffffff",
					"background": "#4063B3",
					"properties": {
						"prefix": " \uE738 "
					}
				},
				{
					"type": "python",
					"style": "powerline",
					"powerline_symbol": "ÓÇ∞",
					"foreground": "#ffffff",
					"background": "#1FB2FF",
					"properties": {
						"prefix": " \uE235 "
					}
				},
				{
					"type": "go",
					"style": "powerline",
					"powerline_symbol": "ÓÇ∞",
					"foreground": "#111111",
					"background": "#8ED1F7",
					"properties": {
						"prefix": " \uE626 ",
						"display_version": true
					}
				},
				{
					"type": "julia",
					"style": "powerline",
					"powerline_symbol": "ÓÇ∞",
					"foreground": "#111111",
					"background": "#4063D8",
					"properties": {
						"prefix": " \uE624 ",
						"display_version": true
					}
				},
				{
					"type": "ruby",
					"style": "powerline",
					"powerline_symbol": "ÓÇ∞",
					"foreground": "#ffffff",
					"background": "#AE1401",
					"properties": {
						"prefix": " \uE791 ",
						"display_version": true,
						"display_mode": "files"
					}
				},
				{
					"type": "azfunc",
					"style": "powerline",
					"powerline_symbol": "ÓÇ∞",
					"foreground": "#ffffff",
					"background": "#FEAC19",
					"properties": {
						"prefix": " \uf0e7",
						"display_version": false,
						"display_mode": "files"
					}
				},
				{
					"type": "aws",
					"style": "powerline",
					"powerline_symbol": "ÓÇ∞",
					"foreground": "#ffffff",
					"background_templates": [
						"{{if contains \"default\" .Profile}}#FFA400{{end}}",
						"{{if contains \"jan\" .Profile}}#f1184c{{end}}"
					],
					"properties": {
						"prefix": " \uE7AD ",
						"display_default": false
					}
				},
				{
					"type": "ytm",
					"style": "powerline",
					"powerline_symbol": "ÓÇ∞",
					"foreground": "#ffffff",
					"background": "#FF0000",
					"properties": {
						"prefix": "\uF16A ",
						"playing_icon": "\uE602 ",
						"paused_icon": "\uF8E3 ",
						"stopped_icon": "\uF04D ",
						"track_separator": " - "
					}
				},
				{
					"type": "battery",
					"style": "powerline",
					"powerline_symbol": "ÓÇ∞",
					"foreground": "#193549",
					"background": "#ffeb3b",
					"properties": {
						"battery_icon": "üîã",
						"discharging_icon": "\uE231 ",
						"charging_icon": "\uE234 ",
						"charged_icon": "\uE22F ",
						"color_background": true,
						"charged_color": "#4caf50",
						"charging_color": "#40c4ff",
						"discharging_color": "#ff5722",
						"postfix": "\uF295 ",
						"display_charging": true,
						"display_charged": true,
						"always_enable": true
					}
				},
				{
					"type": "shell",
					"style": "powerline",
					"powerline_symbol": "ÓÇ∞",
					"foreground": "#ffffff",
					"background": "#203248",
					"properties": {
						"prefix": " \uFCB5 "
					}
				},
				{
					"type": "root",
					"style": "powerline",
					"powerline_symbol": "ÓÇ∞",
					"foreground": "#111111",
					"background": "#ffff66",
					"properties": {
						"root_icon": "\uF0E7"
					}
				},
				{
					"type": "exit",
					"style": "powerline",
					"powerline_symbol": "ÓÇ∞",
					"foreground": "#ffffff",
					"background": "#2e9599",
					"leading_diamond": "",
					"trailing_diamond": "",
					"properties": {
						"always_enabled": true,
						"color_background": true,
						"display_exit_code": false,
						"error_color": "#f1184c",
						"prefix": " Óà∫"
					}
				}
			]
		},
		{
			"type": "prompt",
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"type": "text",
					"style": "plain",
					"foreground": "#007ACC",
					"properties": {
						"prefix": "",
						"text": "\u276F"
					}
				}
			]
		}
	]
}
